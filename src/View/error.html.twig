{% extends "base.html.twig" %}

{% block title%}{{ parent() }} - Erreur !!{% endblock %}

{% block body %}
    <h1 style="color: red">Erreur</h1>
    <div class="alert alert-danger" role="alert">
        <h4 class="alert-heading">{{ code }}</h4>
        <p>{{ message }}</p>
    </div>
{% endblock %}